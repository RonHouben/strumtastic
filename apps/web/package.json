    "exercise-engine": "*",
    "formik": "^2.4.2",
    "lucide-react": "^0.244.0",
    "music-notes": "*",
    "next": "latest",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-opensheet-music-display": "*",
    "request": "^2.88.2",
