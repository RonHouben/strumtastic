{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [],
      "outputs": [
        "dist/**"
      ]
    },
    "audio-engine:build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "database:build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "database#seed": {
      "dependsOn": [
        "database#migrate:push"
      ]
    },
    "database#migrate:push": {
      "dependsOn": [
        "database#start"
      ]
    },
    "database#start": {},
    "web:build": {
      "dependsOn": [
        "audio-engine:build"
      ],
      "env": [
        "VERCEL_URL",
        "PORT",
        "NODE_ENV"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "database#seed"
      ]
    }
  }
}